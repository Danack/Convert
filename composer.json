{
    "name": "danack/convert",
    "description": "Converts between objects and scalars trivially.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "require": {
      "ext-json": "*"
    },
    "require-dev": {
        "danack/coding-standard": "0.1.0",
        "phpunit/phpunit": "9.0.1",
        "slevomat/coding-standard": "^4.8.4",
        "squizlabs/php_codesniffer": "^3.3.1",
        "yoast/yoastcs": "1.0"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-0": {
            "Convert": [
                "./src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "ConvertTest": [
                "./test/"
            ]
        }
    }
